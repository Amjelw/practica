unit UItemsCbx;

interface

type TTipoDoc = class
  private
    Fnombre: string;
    FValor: Integer;
  public
    constructor Create( nombre: string; valor: Integer );
    property nombre:     string read Fnombre write Fnombre;
    property valor:      Integer read FValor write FValor;
end;

implementation

{ TTipoDoc }

constructor TTipoDoc.Create( nombre: string; valor: Integer );
begin
     Self.Fnombre:= nombre;
     Self.FValor:= valor;
end;

end.
